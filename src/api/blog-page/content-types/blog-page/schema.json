{
  "kind": "collectionType",
  "collectionName": "blog_pages",
  "info": {
    "singularName": "blog-page",
    "pluralName": "blog-pages",
    "displayName": "BlogPage"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "topBackground": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "nav_menu": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::nav-menu.nav-menu"
    },
    "blogs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog.blog",
      "mappedBy": "blog_pages"
    }
  }
}
